<body>
{{> mobile_nav}}
{{> nav_grey}}
  <div class="container-fluid full-bg">
    <div class="row" >
      <div class="col-md-2"></div>
      <div class="col-md-8" style="background-color:rgba(255, 255, 255, 0.85);border-radius: 3px;margin-top:10vh">
        <form action="/submit-contact" method="post" role="form" style="height:100%;margin-bottom:5%">


          <div class="row" style="text-align:center">
            <h4 style="font-family:'Times New Roman', Times, serif">{{thankyou}}</h4>
            <h3 style="font-family:'Times New Roman', Times, serif"><b>Contact<b></h3>
          </div>
          <div class="form-group row" style="height:8%;">
            <div class="col-md-1"></div>
            <div class="col-md-10">
              <label for="email">Email address:</label>
              <input type="email" class="form-control" name="email" id="email" required>
            </div>

          </div>
          <div class="form-group row" style="height:8%">
            <div class="col-md-1"></div>
            <div class="col-md-10">
              <label for="name">Name:</label>
              <input type="text" class="form-control" name="name" id="name" required>
            </div>
          </div>
          <div class="form-group row" style="height:60%">
            <div class="col-md-1"></div>
            <div class="col-md-10">
              <label for="message">Message:</label>
              <textarea class="form-control" name="message" style="height:40vh" id="message" required></textarea>
            </div>
          </div>
          <div style="text-align:center"><button type="submit" class="btn btn-primary">Submit</button></div>

        </form>
</div>
    </div>
  </div>
{{> footer}}
</body>
